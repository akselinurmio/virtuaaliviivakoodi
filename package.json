{
  "name": "virtuaaliviivakoodi",
  "version": "2.0.0",
  "description": "Creates the numeral value of Finnish bank transfer barcode",
  "keywords": [
    "pankkiviivakoodi",
    "virtuaaliviivakoodi",
    "bank transfer",
    "wire transfer"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.cjs"
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "prettier": "^3.8.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --cjsInterop --clean --dts --format cjs,esm",
    "check-formatting": "prettier --check .",
    "test": "npm run build && node --test 'test/index.test.js' 'test/index.test.cjs'",
    "test:internals": "node --test 'test/methods.test.js'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akselinurmio/virtuaaliviivakoodi.git"
  },
  "author": "Akseli Nurmio <akseli@nurmio.fi> (https://akselinurmio.fi)",
  "license": "MIT",
  "homepage": "https://github.com/akselinurmio/virtuaaliviivakoodi",
  "bugs": "https://github.com/akselinurmio/virtuaaliviivakoodi/issues",
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
