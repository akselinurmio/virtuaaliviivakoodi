{
  "name": "virtuaaliviivakoodi",
  "version": "2.0.0",
  "description": "Creates the numeral value of Finnish bank transfer barcode",
  "keywords": [
    "pankkiviivakoodi",
    "bank transfer",
    "wire transfer"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.cjs"
  },
  "files": [
    "./dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "tap": "^21.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --cjsInterop --clean --dts --format cjs,esm",
    "check-formatting": "prettier --check .",
    "test": "tap 'src/*.test.js'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akselinurmio/virtuaaliviivakoodi.git"
  },
  "author": "Akseli Nurmio <akseli@nurmio.fi> (https://akselinurmio.fi)",
  "license": "MIT",
  "homepage": "https://github.com/akselinurmio/virtuaaliviivakoodi",
  "bugs": "https://github.com/akselinurmio/virtuaaliviivakoodi/issues",
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
